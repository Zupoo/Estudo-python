import PyPDF2 as pyf
from pathlib import Path

arquivo_pdf = pyf.PdfReader('modulo25/pdf/MGLU_ER_3T20_POR.pdf')

for i, pagina in enumerate(arquivo_pdf.pages):
    num_pagina = i + 1
    novo_pdf = pyf.PdfWriter()
    novo_pdf.add_page(pagina)
    with Path(f'modulo25/pdf/paginas/Arquivo Pagina {num_pagina}.pdf').open(mode = 'wb') as arquivo:
        novo_pdf.write(arquivo)